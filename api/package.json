{
  "name": "vudl-api",
  "private": true,
  "engine": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "format": "prettier --check --write *.js src/**",
    "lint": "eslint *.js src/ --ext .ts,.js,.jsx --max-warnings 0",
    "start:serverWatch": "nodemon --watch 'vudl.ini' --watch '**/*.ts' --watch '**/*.ejs' --ignore node_modules/ src/server.ts",
    "start:workerWatch": "nodemon --watch 'vudl.ini' --watch '**/*.ts' src/worker-start.ts",
    "start:server": "ts-node src/server.ts",
    "start:worker": "ts-node src/worker-start.ts",
    "saml:metadata": "ts-node src/saml-metadata.ts",
    "test": "jest --coverage --no-cache",
    "testWatch": "jest --watch --coverage",
    "watch": "npx tsc --watch"
  },
  "dependencies": {
    "@node-saml/passport-saml": "^4",
    "@stomp/stompjs": "^6.1.0",
    "@xmldom/xmldom": "^0.7.0",
    "body-parser": "^1.20.2",
    "bull-arena": "^3.29.3",
    "bullmq": "^1.14.8",
    "connect-session-knex": "^3.0.1",
    "cookie-parser": "^1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3",
    "express": "^4.18.2",
    "express-session": "^1",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^2.1.1",
    "glob": "^7.1.6",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2",
    "ini": "^1.3.5",
    "knex": "^2.4.2",
    "locutus": "^2",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "n3": "^1",
    "nanoid": "^3.1.23",
    "needle": "^2.6.0",
    "node-tesseract-ocr": "^2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.13.0",
    "sharp": "^0.33.0",
    "sqlite3": "^5",
    "tmp": "^0.2.1",
    "ts-node": "^10",
    "winston": "^3",
    "xml-escape": "^1.1.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-plugin-jsdoc": "^43",
    "eslint-plugin-prettier": "^5",
    "jest": "^29",
    "nodemon": "^3",
    "prettier": "^3",
    "supertest": "^6",
    "ts-jest": "^29",
    "typescript": "^5"
  }
}
